  .PageLayout.PageLayout--hasPaneDivider.PageLayout--panePos-start:not(.PageLayout--columnGap-none) .PageLayout-content { margin-left: var(--Layout-column-gap); }
  .PageLayout.PageLayout--hasPaneDivider.PageLayout--panePos-end .PageLayout-pane { border-left: 1px solid var(--borderColor-default, var(--color-border-default)); }
